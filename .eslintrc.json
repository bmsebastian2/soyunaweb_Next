{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "plugin:@next/next/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier":[
            "error"
        ],
        "react/react-in-jsx-scope":"off",
        "react/prop-types":"off",
        "react/jsx-props-no-spreading":"off",
        "no-unused-vars":"warn"

    }
}
